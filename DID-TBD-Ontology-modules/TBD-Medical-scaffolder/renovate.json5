{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)",
    ":semanticCommitScope(deps)",
    "group:allNonMajor",
    "schedule:earlyMondays", // Run once a week.
  ],
  packageRules: [
    {
      matchPackageNames: ["github.com/TBD54566975/scaffolder"],
      matchManagers: ["gomod"],
      enabled: false,
    },
  ],
}
